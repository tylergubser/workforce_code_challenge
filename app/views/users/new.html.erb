<h1>New User</h1>

<%= form_for @user do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %><br>
    <%= f.label :email_address %>
    <%= f.text_field :email_address %><br>
    <%= f.lable :password %>
    <%= f.password_field :password %><br>
    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %><br>
    <%= f.submit "Create Account"%>
<% end %>

